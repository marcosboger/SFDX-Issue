<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>Users_Shared_With_Admins</fullName>
        <accessLevel>Edit</accessLevel>
        <description>sharing users for all admins to view and modify</description>
        <label>Users Shared With Admins</label>
        <sharedTo>
            <role>CEO</role>
        </sharedTo>
        <criteriaItems>
            <field>Username</field>
            <operation>notEqual</operation>
            <value>Testing</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
